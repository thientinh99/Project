@model TheNight_JustBuy.Models.Feedback

@{
    ViewBag.Title = "Feedback Detail";
    ViewBag.Function = "Feedbacks";
    ViewBag.ChildFunction = "Feedback Detail";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<section class="content">
    <div class="card">
        <div class="card-header">
            <h1 style="font-weight:bold; font-size:25px;" class="card-title">Detail of Feedback</h1>

            <div class="card-tools">
                <a class="btn btn-block btn-secondary float-right" href="~/Admin/Feedbacks">Back to List</a>
            </div>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-12 col-md-12 col-lg-12 order-12 order-md-12">
                    <div class="row">
                        <div class="col-12">
                            <div class="post">
                                <div class="col-sm-2 user-block">
                                    <strong>@Html.DisplayNameFor(model => model.FeedbackID)</strong>:
                                </div>
                                <p class="col-sm-10">
                                    @Html.DisplayFor(model => model.FeedbackID)
                                </p>
                            </div>

                            <div class="post">
                                <div class="col-sm-2 user-block">
                                    <strong>@Html.DisplayNameFor(model => model.FullName)</strong>:
                                </div>
                                <p class="col-sm-10">
                                    @Html.DisplayFor(model => model.FullName)
                                </p>
                            </div>
                            <div class="post">
                                <div class="col-sm-2 user-block">
                                    <strong>@Html.DisplayNameFor(model => model.Email)</strong>:
                                </div>
                                <p class="col-sm-10">
                                    @Html.DisplayFor(model => model.Email)
                                </p>
                            </div>
                            <div class="post">
                                <div class="col-sm-2 user-block">
                                    <strong>@Html.DisplayNameFor(model => model.Subject)</strong>:
                                </div>
                                <p class="col-sm-10">
                                    @Html.DisplayFor(model => model.Subject)
                                </p>
                            </div>
                            <div class="post">
                                <div class="col-sm-2 user-block">
                                    <strong>@Html.DisplayNameFor(model => model.Content)</strong>:
                                </div>
                                <p class="col-sm-10">
                                    @Html.DisplayFor(model => model.Content)
                                </p>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</section>